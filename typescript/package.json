{
  "name": "skeleton-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=production nodemon",
    "build": "npm run lint && tsc -p .",
    "start:local": "npm run lint && cross-env NODE_ENV=local nodemon",
    "start:test": "npm run lint && cross-env NODE_ENV=test nodemon build/server.js",
    "start:staging": "npm run lint && cross-env NODE_ENV=staging node build/server.js",
    "start:production": "npm run lint && cross-env NODE_ENV=production build/server.js",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pretty": "prettier --write 'src/**/*.ts'",
    "precommit": "npm run lint-fix && npm run pretty",
    "prepush": "npm run lint ",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^13.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
